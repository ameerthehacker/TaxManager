{
  "name": "tax-manager-electron",
  "version": "1.0.0",
  "description": "It is a tax management electron app for the panchayat",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package:win":
      "electron-packager . TaxManager --platform=win32 --overwrite --out=./dist",
    "express:start": "nodemon ./express/app.js",
    "electron:rebuild": "electron-rebuild -f -w sqlite3"
  },
  "keywords": ["electron", "tax", "panchayat"],
  "author": "ameerthehacker",
  "license": "MIT",
  "devDependencies": {
    "electron": "^1.8.2",
    "electron-packager": "^11.0.1",
    "electron-rebuild": "^1.7.3",
    "nodemon": "^1.15.0"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.1.1",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "sqlite3": "^3.1.13"
  }
}
